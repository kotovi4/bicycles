  "homepage": "https://kotovi4.github.io/bicycles",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "^10.4.0",
    "browser-sync": "2.26.x",
    "eslint": "4.16.0",
    "eslint-config-htmlacademy": "0.4.x",
    "gh-pages": "^3.2.3",
    "gulp": "4.0.x",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^5.0.3",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "9.0",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "gulp-svgstore": "^7.0.1",
    "gulp-webp": "^4.0.1",
    "imask": "^6.2.2",
    "node": "^14.15.0",
    "pixel-glass": "^1.0.13",
    "postcss": "^8.2.8",
    "postcss-csso": "^5.0.1",
    "postcss-reporter": "^7.0.2",
    "posthtml-include": "^1.2.0",
    "stylelint": "^13.13.1",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint && eslint source/js/",
    "build": "gulp build",
    "start": "gulp start",
    "webp": "gulp webp",
    "sprite": "gulp sprite",
    "deploy": "gh-pages -d build",
